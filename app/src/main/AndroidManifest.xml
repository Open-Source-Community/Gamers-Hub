<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.an.gamers">

    <application
        android:allowBackup="true"
        android:icon="@drawable/gamers_hub_icn"
        android:label="@string/app_name"
        android:roundIcon="@drawable/gamers_hub_icn"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.NoActionBar">
        <activity android:name=".Game_Profile"></activity>
        <activity android:name=".AdminstrationLayouts.AdminMain" />
        <activity android:name=".User_Profile.User_Profile" />
        <activity
            android:name=".Opener.Opener"
            android:screenOrientation="portrait" />
        <activity android:name=".MainActivity" />
        <activity android:name=".Signing.SigningActivity" />
        <activity
            android:name=".Opener.SplashScreen"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>